class Solution {
   public int countDigits(int num) {
       int count =0;
       int newNum=num;
       for(int i = 0; i<=num; i++) {
           if(newNum<1){
               break;
           }
           int val = newNum%10;
           if (num % val == 0) {
               newNum = newNum / 10;
               count++;
           }
           else{
               newNum = newNum / 10;
           }


       }
       return count;


   }
}
